{
  "name": "operator",
  "version": "3.30.0",
  "description": "Operator - Advanced medical AI assistant Chrome extension with specialized agents and EMR integration",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-with-types": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:verbose": "node run-e2e-tests.js --verbose",
    "validate:production": "npm run build && npm run type-check && npm run lint:src",
    "validate:full": "npm run build && npm run type-check && npm run lint:src && npm run test:e2e",
    "lint:src": "eslint src/**/*.{ts,tsx} --max-warnings 30",
    "dspy:server:start": "./start-dspy-server.sh start",
    "dspy:server:stop": "./start-dspy-server.sh stop",
    "dspy:server:restart": "./start-dspy-server.sh restart",
    "dspy:server:status": "./start-dspy-server.sh status",
    "dspy:server:logs": "./start-dspy-server.sh logs",
    "dspy:server:health": "./start-dspy-server.sh health",
    "dev:start": "./dev",
    "dspy:auto-start": "./dev",
    "eval:angiogram": "USE_DSPY=true dspy-env/bin/python -m llm.evaluate --task angiogram-pci",
    "eval:quick-letter": "USE_DSPY=true dspy-env/bin/python -m llm.evaluate --task quick-letter",
    "eval:tavi": "USE_DSPY=true dspy-env/bin/python -m llm.evaluate --task tavi",
    "eval:consultation": "USE_DSPY=true dspy-env/bin/python -m llm.evaluate --task consultation",
    "eval:investigation-summary": "USE_DSPY=true dspy-env/bin/python -m llm.evaluate --task investigation-summary",
    "optim:angiogram": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task angiogram-pci --iterations 5",
    "optim:quick-letter": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task quick-letter --iterations 5",
    "optim:tavi": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task tavi --iterations 5",
    "optim:consultation": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task consultation --iterations 3",
    "optim:angiogram:human": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task angiogram-pci --iterations 5 --with-human",
    "optim:quick-letter:human": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task quick-letter --iterations 5 --with-human",
    "optim:tavi:human": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task tavi --iterations 5 --with-human",
    "optim:quick-letter:with-exemplars": "USE_DSPY=true dspy-env/bin/python -m llm.optim_gepa --task quick-letter --iterations 5 --with-exemplars"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/rapier": "^1.5.0",
    "@tanstack/react-query": "^5.85.5",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@xenova/transformers": "^2.10.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.300.0",
    "meshline": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-loading-indicators": "^1.0.1",
    "three": "^0.168.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/chrome": "^0.0.258",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "medical",
    "ai",
    "chrome-extension",
    "healthcare",
    "dictation",
    "emr"
  ],
  "author": "Operator Team",
  "license": "MIT"
}
